<h1>followings</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@following_users) do |f| %>
    <%= render 'shared/error_messages', object: f.object %>
   
      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %>

      <%= f.label :email %>
      <%= f.text_field :email, class: 'form-control' %>
    <% end %>
  </div>
</div>